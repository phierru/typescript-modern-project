{
  "name": "tsmodules",
  "version": "1.1.0",
  "description": "TypeScript project template to import modules without extension, and modules with default exports that use node built-ins",
  "keywords": [
    "typescript",
    "modules",
    "es modules",
    "esnext",
    "influxdb",
    "apollo-server"
  ],
  "license": "MIT",
  "author": "dandv",
  "type": "module",
  "main": "run.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -f *.js *.js.map",
    "lint": "eslint *.ts",
    "start": "node --experimental-specifier-resolution=node --harmony -r source-map-support/register build/index.js",
    "dev": "npm run build && node --experimental-specifier-resolution=node --harmony -r source-map-support/register build/index.js | pino-pretty -t",
    "test": "node --harmony node_modules/.bin/jest"
  },
  "dependencies": {
    "activedirectory2": "^2.1.0",
    "dotenv": "^8.2.0",
    "influx": "^5.6.0",
    "mssql": "^6.3.1",
    "pino": "^6.11.1"
  },
  "devDependencies": {
    "@types/activedirectory2": "^1.2.2",
    "@types/jest": "^26.0.7",
    "@types/mssql": "^6.0.7",
    "@types/node": "^14.0.26",
    "@types/pino": "^6.3.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-plugin-jest": "^23.18.2",
    "jest": "^26.1.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=13.0.0"
  }
}
